<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.custocar.custocar">

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".TelaInicialActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".CadastroActivity" />
        <activity android:name=".UsuarioActivity" />
        <activity android:name=".RelatorioActivity" />
        <activity android:name=".DespesasActivity" />
        <activity android:name=".CombustivelActivity" />
        <activity android:name=".ImprevistosActivity" />
        <activity android:name=".ServicosActivity" />
        <activity android:name=".CadastrarVeiculoActivity" />
        <activity android:name=".CalendarioActivity"></activity>
    </application>

</manifest>